package firstProject;

import java.util.Scanner;

public class HelloWorld {
	
	public static Scanner input = new Scanner(System.in);
	
	public static void main(String[] args) {
		int num = input.nextInt();
		for (int i = 1; i <= num; i++){
			for (int j = 0; j < i; j++){
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
